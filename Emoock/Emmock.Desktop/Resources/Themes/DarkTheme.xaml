<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
					xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

	<!-- Color Brushes -->
	<Color x:Key="PrimaryColor"
		   R="1"
		   G="173"
		   B="149"
		   A="255" />
	
	<Color x:Key="PrimarySelectedColor"
		   R="1"
		   G="173"
		   B="149"
		   A="64" />

	<SolidColorBrush x:Key="Primary"
					 Color="{StaticResource PrimaryColor}" />

	<SolidColorBrush x:Key="PrimarySelected"
					 Color="{StaticResource PrimarySelectedColor}" />

	<SolidColorBrush x:Key="Background"
					 Color="#132134" />
	
	<SolidColorBrush x:Key="BackgroundGrey"
					 Color="#F5F5F5" />

	<SolidColorBrush x:Key="MediumGray"
					 Color="#C1C6C8" />
	
	<SolidColorBrush x:Key="LightGray"
					 Color="#C5CED3" />
	
	<SolidColorBrush x:Key="DarkGray"
					 Color="#787878" />
	
	<SolidColorBrush x:Key="Transparent"
					 Color="Transparent" />

	<SolidColorBrush x:Key="PrimaryFontColor"
					 Color="#333333" />
	<SolidColorBrush x:Key="SecondaryFontColor"
					 Color="#FFFFFF" />

</ResourceDictionary>